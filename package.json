{
  "name": "Barebare",
  "version": "0.1.0",
  "description": "Get started with React, Redux, React-Router and Webpack.",
  "engines": {
    "node": ">=6.0.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "dev": "better-npm-run dev",
    "start": "better-npm-run start",
    "clean": "rimraf build",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "start": {
      "command": "node bin/server"
    },
    "dev": {
      "command": "nodemon bin/server --ignore dist --ignore coverage --ignore tests",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/test/karma.conf.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Picta-it/barebare.git"
  },
  "author": "John Guignard <john.guignard@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "argv": "0.0.2",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2017": "^6.16.0",
    "better-npm-run": "0.0.11",
    "config": "^1.21.0",
    "css-loader": "^0.25.0",
    "cssnano": "^3.7.4",
    "html-webpack-plugin": "^2.24.0",
