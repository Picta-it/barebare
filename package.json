    "koa": "^1.2.4",
    "koa-compose": "^2.4.0",
    "koa-webpack-dev": "^1.0.6",
    "material-ui": "^0.15.4",
    "react": "^15.3.2",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.0.0",
